    -->
    <!-- [END_EXCLUDE] -->
    <!--
      A web.xml is needed to explicitly set the order in which filters process requests. Any filters
      not included in web.xml will be loaded after filters listed below.
    -->
    <!-- [START filters] -->
    <filter-mapping>
        <filter-name>DatastoreSessionFilter</filter-name>
    </filter-mapping>
    <filter-mapping>
        <filter-name>ListByUserFilter</filter-name>
    </filter-mapping>
    <!-- [END filters] -->
    <!-- [START config] -->
    <context-param>
        <param-name>location.callback</param-name>
        <param-value>${location.callback}</param-value>
    </context-param>
    <context-param>
        <param-name>location.storageType</param-name>
        <param-value>${location.storageType}</param-value>
    </context-param>
    <context-param>
        <param-name>location.bucket</param-name>
        <param-value>${location.bucket}</param-value>
    </context-param>
    <context-param>
        <param-name>location.clientID</param-name>
        <param-value>${location.clientID}</param-value>
    </context-param>
    <context-param>
        <param-name>location.clientSecret</param-name>
        <param-value>${location.clientSecret}</param-value>
    </context-param>
    <!-- [END config] -->
    <context-param>
        <param-name>sql.url</param-name>
        <param-value>jdbc:mysql://${sql.host}:3306/${sql.dbName}?user=${sql.userName}&amp;password=${sql.password}</param-value>
    </context-param>
    <!-- [START welcome] -->
    <welcome-file-list>
        <welcome-file>/locations</welcome-file>
    </welcome-file-list>
    <!-- [END welcome] -->
</web-app>
<!-- [END webxml] -->
